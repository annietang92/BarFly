<script type="text/javascript">
  // Set up typeahead
  var setUpLocationTypeahead = function() {
      // Initialize typeahead.
      $(".location_tt").typeahead({
          name: 'location',
          local: <%= @locations.to_s.html_safe %>
      });
  }

  $(document).ready(function(){
    setUpLocationTypeahead();
  });
</script>

<script type="text/javascript">
  // Set up typeahead
  var setUpDrinksTypeahead = function() {
      // Initialize typeahead.
      $(".drinks_tt").typeahead({
          name: 'drinks',
          local: <%= @all_drinks.to_s.html_safe %>
      });
  }
  $(document).ready(function(){
    setUpDrinksTypeahead();
  });
</script>